  <section class="md:w-2/3 mx-auto">
    <div class="px-6 py-4 mx-auto mt-8 lg:-mx-6 lg:flex lg:items-center">
      <div class="w-full lg:mx-6 lg:w-1/2 rounded-xl">
        <%
        query = ""
        if @hike.trailhead_address.include?('place/')
          query = @hike.trailhead_address.split('place/').last.split('/').first
        elsif @hike.trailhead_address.include?('q=')
          query = @hike.trailhead_address.split('q=').last.split('&').first
        elsif @hike.trailhead_address.include?('@')
          query = @hike.trailhead_address.split('@').last.split(',').first
        elsif @hike.trailhead_address.include?('data=')
          query = @hike.trailhead_address.split('data=').last.split('!').first 
        end

        # Construct embed URL with API key
        embed_url = 'https://www.google.com/maps/embed/v1/place' + '?key=' + ENV['GOOGLE_MAPS_API_KEY'] 
        embed_url += '&q=' + query
      %>
        <iframe src="<%= embed_url %>" width="100%" height="450" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
      </div>
      <div class="mt-2 lg:w-1/2 lg:mt-0 lg:mx-6">
        <div class="block text-xl font-semibold text-gray-800">
          Suggested Items
        </div>
        <div class="mt-3 space-y-3">
          <% hike.suggested_items.split(',').each do |item| %>
            <div class="flex items-center space-x-2">
              <input type="checkbox" class="h-4 w-4 rounded border-gray-300 text-primary-600 shadow-sm focus:border-primary-300 focus:ring focus:ring-primary-200 focus:ring-opacity-50 focus:ring-offset-0 disabled:cursor-not-allowed disabled:text-gray-400" />
              <label for="example1" class="text-sm font-medium text-gray-700"><%= item.strip.capitalize %></label>
            </div>
          <% end %>
        </div>
        <div class="mt-6 block text-xl font-semibold text-gray-800">
          Notes/Instructions
        </div>
        <div class="mt-3 flex overflow-hidden text-md text-gray-500">
          <%= @hike.notes %>
        </div>
      </div>
    </div>
  </section>
</div>